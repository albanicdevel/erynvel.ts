{
  "name": "erynvel",
  "module": "/package/index.ts",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "repository": {
    "url": "https://github.com/albanicdevel/erynvel.ts"
  },
  "exports": {
    ".": {
